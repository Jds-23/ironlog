{
  "name": "@ironlog/db",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./schema": {
      "default": "./src/schema/index.ts"
    },
    "./client": {
      "default": "./src/client.ts"
    },
    "./local-migrations/migrations": {
      "types": "./src/local-migrations/migrations.d.ts",
      "default": "./src/local-migrations/migrations.js"
    },
    "./*": {
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:generate:local": "drizzle-kit generate --config=drizzle.config.expo.ts"
  },
  "dependencies": {
    "@ironlog/env": "workspace:*",
    "@libsql/client": "0.15.15",
    "dotenv": "catalog:",
    "drizzle-orm": "^0.45.1",
    "libsql": "0.5.22",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@ironlog/config": "workspace:*",
    "drizzle-kit": "^0.31.8",
    "typescript": "catalog:"
  }
}
