{
  "expo": {
    "scheme": "ironlog",
    "userInterfaceStyle": "dark",
    "orientation": "portrait",
    "backgroundColor": "#0a0a0a",
    "web": {
      "bundler": "metro"
    },
    "name": "IronLog",
    "slug": "ironlog",
    "plugins": ["expo-font", "expo-router"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "android": {
      "permissions": ["VIBRATE"]
    }
  }
}
